<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="112dp" />
            <gradient
                android:angle="270"
                android:endColor="#FED000"
                android:startColor="#01FFC1" />
        </shape>
    </item>

    <item
        android:bottom="1.14dp"
        android:left="1.14dp"
        android:right="1.14dp"
        android:top="1.14dp">
        <shape android:shape="rectangle">
            <corners android:radius="112dp" />
            <solid android:color="#0D0D0D" />
        </shape>
    </item>

</layer-list>